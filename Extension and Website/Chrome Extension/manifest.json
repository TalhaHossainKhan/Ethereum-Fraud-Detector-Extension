{
  "manifest_version": 3,
  "name": "Sensic - Crypto Fraud Detector",
  "version": "1.0",
  "description": "Removing doubts in the crypto space.",
  "icons": {
    "128": "icon1281.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "128": "icon1281.png"
    }
  },
  "host_permissions": [
    "https://api.alternative.me/fng/*",
    "https://talhahossainkhan.github.io/sensicapi/addresses-darklist.json",
    "https://api.etherscan.io/api?module=account&action=balance&address=*",
    "https://eth.blockscout.com/api/v2/addresses/*",
    "https://talhahossainkhan.github.io/sensicapi/data.json",
    "https://sensic-url-api.onrender.com/?URL=*",
    "https://sensic-sentinel-mlapi.onrender.com/?AddressEther=*"
  ]
}


